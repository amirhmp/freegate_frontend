{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx" ,
    "baseUrl": "src",
    "paths": {
      "@assets/*": ["assets/*"],
      "@constants/*": ["constants/*"],
      "@context/*": ["context/*"],
      "@hooks/*": ["hooks/*"],
      "@models/*": ["models/*"],
      "@DTOs/*": ["models/DTOs/*"],
      "@services/*": ["services/*"],
      "@ui/*": ["ui/*"],
      "@pages/*": ["ui/pages/*"],
      "@components/*": ["ui/components/*"],
      "@utils/*": ["utils/*"]
    }
  },
  "include": ["src"]
}
